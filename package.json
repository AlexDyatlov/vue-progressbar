{
  "name": "vue-progressbar",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "check": "concurrently \"npm run type:check\" \"npm run lint\" \"npm run format:check\" \"npm run audit\"",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "lint": "eslint src --max-warnings 0",
    "type:check": "vue-tsc --noEmit --incremental false -p tsconfig.app.json",
    "audit": "npm audit --audit-level=high"
  },
  "dependencies": {
    "vue": "3.5.21"
  },
  "devDependencies": {
    "@types/node": "24.6.0",
    "@vitejs/plugin-vue": "6.0.1",
    "@vue/tsconfig": "0.8.1",
    "concurrently": "9.2.1",
    "eslint": "9.36.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-vue": "10.5.0",
    "globals": "16.4.0",
    "prettier": "3.6.2",
    "sass": "1.93.2",
    "typescript": "5.8.3",
    "typescript-eslint": "8.44.1",
    "vite": "7.1.7",
    "vue-tsc": "3.0.7"
  }
}